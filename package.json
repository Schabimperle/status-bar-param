{
	"name": "status-bar-parameter",
	"displayName": "status_bar_parameter",
	"description": "Add customizable parameters to the status bar.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.42.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "Status Bar Params Configuration",
			"properties": {
				"statusbar.params": {
					"type": "array",
					"default": [],
					"description": "Parameters",
					"items": {
						"type": "object",
						"title": "Parameter",
						"properties": {
							"parameter": {
								"type": "string",
								"description": "Name of the Parameter"
							},
							"strings": {
								"type": "array",
								"description": "Selectable strings",
								"items": {
									"type": "string",
									"title": "Selectable string"
								}
							}
						}
					}
				}
			}
		},
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.42.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.18.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	}
}